{
  rules: [
    {
      files: [".pre-commit-config.yaml"],
      JMESPathsMatch: [
        [
          "length(repos[?repo=='https://github.com/pre-commit/pre-commit-hooks'])",
          1,
        ],
        [
          // the 'rev' key must match the vX.Y.Z regex pattern
          "regex_match('^v\\d+\\.\\d+\\.\\d+$', repos[?repo=='https://github.com/pre-commit/pre-commit-hooks'] | [0].rev)",
          true,
        ],
        [
          "contains(repos[?repo=='https://github.com/pre-commit/pre-commit-hooks'] | [0].hooks[*].id, 'double-quote-string-fixer')",
          true,
        ],
        [
          "repos[?repo=='https://github.com/pre-commit/pre-commit-hooks'] | [0].hooks[?id=='double-quote-string-fixer'] | [0].name",
          "double-quote-string-fixer",
        ],
      ],
    },
  ],
}
