{
  rules: [
    {
      // Use prettier to format YAML files
      files: [".pre-commit-config.yaml"],
      JMESPathsMatch: [
        [
          "contains(repos[?repo=='https://github.com/pre-commit/mirrors-prettier'] | [0].hooks[0].types_or, 'yaml')",
          true,
        ],
      ],
    },
  ],
}
